{
    "family": "s3-interaction-task",
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::317327656978:role/s3-ecs-task-role",
    "containerDefinitions": [
        {
            "name": "s3-interaction-container",
            "image": "317327656978.dkr.ecr.us-east-1.amazonaws.com/ecs-ecr-repo:latest",
            "cpu": 256,
            "memory": 512,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "environment": [
                {
                    "name": "S3_BUCKET_NAME",
                    "value": "ecs-bucket-test-1"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/s3-interaction-task",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}